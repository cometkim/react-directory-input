{
  "name": "react-directory-input",
  "version": "1.1.0",
  "description": "A simple component to handle a directory as a single file",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "react",
    "react-dom",
    "form"
  ],
  "source": "./src/index.ts",
