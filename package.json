    "build": "nanobundle build"
  },
  "files": [
    "src",
    "lib"
  ],
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@cometjs/react-utils": "^2.0.3",
    "@zip.js/zip.js": "^2.4.15",
    "react-merge-refs": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "nanobundle": "^0.0.28",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.7.3"
  },
  "packageManager": "yarn@3.2.1"
